{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_widget(form) }}
    <div>
        <button class="btn btn-default" type="submit" value="{{ 'registration.submit'|trans }}"><span class="glyphicon glyphicon-ok-circle" aria-hidden="true"></span> Registrar</button>
        
    </div>
</form>
